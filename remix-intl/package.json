{
  "name": "remix-intl",
  "version": "0.0.6",
  "description": "Internationalization(i18n) library for your Remix apps",
  "author": "suhaotian",
  "license": "MIT",
  "homepage": "https://remix-intl.tskd.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/suhaotian/remix-intl"
  },
  "main": "./index.cjs",
  "types": "./index.d.ts",
  "require": "./index.cjs",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "import": "./index.mjs",
      "module": "./index.esm.js"
    },
    "./i18n": {
      "types": "./i18n.d.ts",
      "require": "./i18n.cjs",
      "import": "./i18n.mjs",
      "module": "./i18n.esm.js"
    },
    "./navigation": {
      "types": "./navigation.d.ts",
      "require": "./navigation.cjs",
      "import": "./navigation.mjs",
      "module": "./navigation.esm.js"
    },
    "./server": {
      "types": "./server.d.ts",
      "require": "./server.cjs",
      "import": "./server.mjs",
      "module": "./server.esm.js"
    },
    "./utils": {
      "types": "./utils.d.ts",
      "require": "./utils.cjs",
      "import": "./utils.mjs",
      "module": "./utils.esm.js"
    },
    "./types": {
      "types": "./types.d.ts",
      "require": "./types.cjs",
      "import": "./types.mjs",
      "module": "./types.esm.js"
    }
  },
  "scripts": {
    "build": "bunchee --external @remix-run/react,@remix-run/node,react,i18next",
    "dev:build": "bunchee -w --external @remix-run/react,@remix-run/node,react,i18next",
    "test": "jest --collectCoverage",
    "typecheck": "tsc",
    "start-publish": "cp ../README.md ./ && pnpm build && node scripts/purge-pkg-for-publish.mjs && npm publish --registry=https://registry.npmjs.org"
  },
  "devDependencies": {
    "i18next": "^23",
    "react": "^18",
    "react-dom": "^18",
    "xior": "^0.3.5",
    "bunchee": "^5.1.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "typescript": "^5",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-jest": "^29.7.0",
    "vite": "^5.1.0",
    "@remix-run/react": "^2.8.1",
    "@remix-run/node": "^2.8.1"
  },
  "files": [
    "*.d.ts",
    "*.cjs",
    "*.mjs",
    "*.esm.js",
    "README.md"
  ],
  "keywords": [
    "remix",
    "i18next",
    "react",
    "intl",
    "i18n",
    "internationalization",
    "localization",
    "translate",
    "translation"
  ]
}
