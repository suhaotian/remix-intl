{
  "name": "remix-intl",
  "version": "0.0.15",
  "description": "Internationalization(i18n) library for your Remix apps",
  "author": "suhaotian",
  "license": "MIT",
  "homepage": "https://remix-intl.tskd.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/suhaotian/remix-intl"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "require": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./i18n": {
      "import": {
        "types": "./i18n.d.mts",
        "default": "./i18n.mjs"
      },
      "require": {
        "types": "./i18n.d.ts",
        "default": "./i18n.js"
      }
    },
    "./navigation": {
      "import": {
        "types": "./navigation.d.mts",
        "default": "./navigation.mjs"
      },
      "require": {
        "types": "./navigation.d.ts",
        "default": "./navigation.js"
      }
    },
    "./server": {
      "import": {
        "types": "./server.d.mts",
        "default": "./server.mjs"
      },
      "require": {
        "types": "./server.d.ts",
        "default": "./server.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./utils.d.mts",
        "default": "./utils.mjs"
      },
      "require": {
        "types": "./utils.d.ts",
        "default": "./utils.js"
      }
    },
    "./types": {
      "import": {
        "types": "./types.d.mts",
        "default": "./types.mjs"
      },
      "require": {
        "types": "./types.d.ts",
        "default": "./types.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "dev:build": "tsup",
    "test": "jest --collectCoverage",
    "typecheck": "tsc",
    "start-publish": "cp ../README.md ./ && pnpm build && node scripts/purge-pkg-for-publish.mjs && npm publish --registry=https://registry.npmjs.org"
  },
  "devDependencies": {
    "i18next": "^23",
    "react": "^19",
    "react-dom": "^19",
    "xior": "^0.7.8",
    "tsup": "^8.4.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "typescript": "^5.8.3",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-jest": "^29.7.0",
    "vite": "^6.3.5",
    "@remix-run/react": "^2.16.6",
    "@remix-run/node": "^2.16.6"
  },
  "files": [
    "*.d.ts",
    "*.js",
    "*.mts",
    "*.mjs",
    "*.esm.js",
    "dist",
    "README.md"
  ],
  "keywords": [
    "remix",
    "i18next",
    "react",
    "react-router",
    "intl",
    "i18n",
    "internationalization",
    "localization",
    "translate",
    "translation"
  ]
}
