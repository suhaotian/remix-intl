{
  "name": "remix-intl",
  "version": "0.0.7",
  "description": "Internationalization(i18n) library for your Remix apps",
  "author": "suhaotian",
  "license": "MIT",
  "homepage": "https://remix-intl.tskd.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/suhaotian/remix-intl"
  },
  "main": "./index.cjs",
  "types": "./index.d.ts",
  "require": "./index.cjs",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "import": "./index.mjs",
      "module": "./index.esm.js"
    },
    "./i18n": {
      "types": "./i18n.d.ts",
      "require": "./i18n.cjs",
      "import": "./i18n.mjs",
      "module": "./i18n.esm.js"
    },
    "./navigation": {
      "types": "./navigation.d.ts",
      "require": "./navigation.cjs",
      "import": "./navigation.mjs",
      "module": "./navigation.esm.js"
    },
    "./server": {
      "types": "./server.d.ts",
      "require": "./server.cjs",
      "import": "./server.mjs",
      "module": "./server.esm.js"
    },
    "./utils": {
      "types": "./utils.d.ts",
      "require": "./utils.cjs",
      "import": "./utils.mjs",
      "module": "./utils.esm.js"
    },
    "./types": {
      "types": "./types.d.ts",
      "require": "./types.cjs",
      "import": "./types.mjs",
      "module": "./types.esm.js"
    }
  },
  "files": [
    "*.d.ts",
    "*.cjs",
    "*.mjs",
    "*.esm.js",
    "README.md"
  ],
  "keywords": [
    "remix",
    "i18next",
    "react",
    "intl",
    "i18n",
    "internationalization",
    "localization",
    "translate",
    "translation"
  ]
}