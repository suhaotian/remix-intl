{
  "name": "remix-intl",
  "version": "0.0.16",
  "description": "Internationalization(i18n) library for your Remix apps",
  "author": "suhaotian",
  "license": "MIT",
  "homepage": "https://remix-intl.tskd.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/suhaotian/remix-intl"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "require": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./i18n": {
      "import": {
        "types": "./i18n.d.mts",
        "default": "./i18n.mjs"
      },
      "require": {
        "types": "./i18n.d.ts",
        "default": "./i18n.js"
      }
    },
    "./navigation": {
      "import": {
        "types": "./navigation.d.mts",
        "default": "./navigation.mjs"
      },
      "require": {
        "types": "./navigation.d.ts",
        "default": "./navigation.js"
      }
    },
    "./server": {
      "import": {
        "types": "./server.d.mts",
        "default": "./server.mjs"
      },
      "require": {
        "types": "./server.d.ts",
        "default": "./server.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./utils.d.mts",
        "default": "./utils.mjs"
      },
      "require": {
        "types": "./utils.d.ts",
        "default": "./utils.js"
      }
    },
    "./types": {
      "import": {
        "types": "./types.d.mts",
        "default": "./types.mjs"
      },
      "require": {
        "types": "./types.d.ts",
        "default": "./types.js"
      }
    }
  },
  "files": [
    "*.d.ts",
    "!babel.config.js",
    "*.js",
    "*.mts",
    "*.mjs",
    "*.esm.js",
    "dist",
    "README.md"
  ],
  "keywords": [
    "remix",
    "i18next",
    "react",
    "react-router",
    "intl",
    "i18n",
    "internationalization",
    "localization",
    "translate",
    "translation"
  ]
}